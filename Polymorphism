class Vehicle:
    def move(self):
        raise NotImplementedError("Subclass must implement abstract method")


class Car(Vehicle):
    def move(self):
        return "Driving ğŸš—"


class Bike(Vehicle):
    def move(self):
        return "Riding a bike ğŸš´â€â™‚ï¸"


class Plane(Vehicle):
    def move(self):
        return "Flying âœˆï¸"


def demonstrate_movement(vehicles):
    for vehicle in vehicles:
        print(vehicle.move())


# Example usage
if __name__ == "__main__":
    # Create instances of each vehicle
    car = Car()
    bike = Bike()
    plane = Plane()

    # List of vehicles
    vehicles = [car, bike, plane]

    # Demonstrate movement
    demonstrate_movement(vehicles)
